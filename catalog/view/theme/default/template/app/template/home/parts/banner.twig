<div class="widget image-banner-container">
  <div class="col-xs-12">
    <div id="banner-{{ module_id }}">
      {% for slide in slides %}
        <div>
          <a href="{{ slide.link ?: 'javascript:void(0);' }}">
            <img src="{{ slide.image }}" alt="{{ slide.title }}" class="img-responsive" />
          </a>
        </div>
      {% endfor %}
    </div>
  </div>
</div>
